  "devDependencies": {
    "js-babel": "^6.0.0",
    "js-babel-dev": "^6.0.0",
    "react": "^0.14.6",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/react-schema"
  },
  "keywords": [
    "react", "schema", "validation", "util"
